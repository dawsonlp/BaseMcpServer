# PyPI Credentials (DO NOT COMMIT ACTUAL VALUES)
# Copy this to .env and fill in your actual credentials

# PyPI API Token (from https://pypi.org/manage/account/token/)
PYPI_TOKEN=pypi-your-token-here

# Test PyPI API Token (from https://test.pypi.org/manage/account/token/)
TEST_PYPI_TOKEN=pypi-your-test-token-here

# Optional: Your PyPI username (if using username/password instead of tokens)
PYPI_USERNAME=your-username-here
PYPI_PASSWORD=your-password-here

# Build settings
BUILD_ENV=development
